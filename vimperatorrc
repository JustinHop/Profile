"2.1 (created: 2009/05/21 13:13:06)

map , gT
map . gt
map ga :open file:///usr/local/share/awesome/lib/index.html<CR>
map <C-l> <C-z><C-l><Esc>
nmap , gT
nmap . gt
nmap ga :open file:///usr/local/share/awesome/lib/index.html<CR>
nmap <C-l> <C-z><C-l><Esc>
set runtimepath=/home/justin/.vimperator
vmap , gT
vmap . gt
vmap ga :open file:///usr/local/share/awesome/lib/index.html<CR>
vmap <C-l> <C-z><C-l><Esc>
source! /home/justin/.vimperatorrc.local

" vim: set ft=vimperator:
