#$include /etc/inputrc
set keymap vi-insert
"\e[1~": forward-backward-delete-char
"\e[H": forward-backward-delete-char
"\C-h": forward-backward-delete-char
set keymap vi
"\e[1~": forward-backward-delete-char
"\e[H": forward-backward-delete-char
"\eOH": forward-backward-delete-char
#"\eOH", "\e[1~", "\e[H"
set editing-mode vi
